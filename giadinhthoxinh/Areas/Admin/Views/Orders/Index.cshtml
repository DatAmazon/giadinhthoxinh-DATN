            @Html.DisplayFor(modelItem => item.fSurcharge)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.iPaid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.iState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblUser.sEmail)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_iOrderID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PK_iOrderID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PK_iOrderID })
        </td>
    </tr>
}

</table>
