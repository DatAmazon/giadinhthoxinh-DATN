            @Html.DisplayNameFor(model => model.tblUser.sEmail)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sCustomerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sCustomerPhone)
