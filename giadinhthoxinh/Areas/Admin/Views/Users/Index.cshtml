@model IEnumerable<giadinhthoxinh.Models.tblUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.sEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sPass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sUserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.iState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblPermission.sPermissionName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sPass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sUserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.iState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblPermission.sPermissionName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_iAccountID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PK_iAccountID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PK_iAccountID })
        </td>
    </tr>
}

</table>
